{
    "name": "ellipse/resolvable-class",
    "type": "library",
    "description": "Resolvable class factory allowing to instantiate classes using a Psr-11 container",
    "keywords": [
        "psr-11",
        "container",
        "dependency-injection",
        "class-dependency-injection"
    ],
    "license": "MIT",
    "homepage": "https://github.com/ellipsephp/resolvable-class",
    "support": {
        "issues": "https://github.com/ellipsephp/resolvable-class/issues",
        "source": "https://github.com/ellipsephp/resolvable-class"
    },
    "authors": [
        {
            "name": "pmall",
            "email": "pierre.mallinjoud@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "psr/container": "^1.0",
        "ellipse/resolvable-value": "^1.0"
    },
    "require-dev": {
        "kahlan/kahlan": "^4.0",
        "eloquent/phony-kahlan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Ellipse\\Resolvable\\": "src"
        }
    }
}
